  "return this":                          { "prefix": "rt",       "body": [ "return this" ],                                   "description": "return this" },
  "return new object":                    { "prefix": "ro",       "body": [ "return {\n\t${0}\n}" ],                           "description": "return new object" },

  "ternary operator":                     { "prefix": "ternary",  "body": [ "${1} ? ${2} : ${3}" ],                            "description": "ternary operator" },

  "alert":                                { "prefix": "al",       "body": [ "alert('${1:msg}');" ],                            "description": "window.alert" },
  "confirm":                              { "prefix": "conf",     "body": [ "confirm('${1:msg}');" ],                          "description": "window.confirm" },
  "prompt":                               { "prefix": "pm",       "body": [ "prompt('${1:msg}');" ],                           "description": "window.prompt" },
