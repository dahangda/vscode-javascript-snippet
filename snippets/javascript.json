  "setInterval, arrow function":          { "prefix": "sia",      "body": [ "setInterval(() => {\n\t${2}\n}, ${0:interval});" ],      "description": "setInterval with arrow function" },

  "console.dir":                          { "prefix": "cd",       "body": [ "console.dir($1);" ],                              "description": "console.dir" },
  "console.error":                        { "prefix": "ce",       "body": [ "console.error($1);" ],                            "description": "console.error" },
  "console.error labeled":                { "prefix": "celb",     "body": [ "console.error('${1}', ${1});" ],                  "description": "console.error labeled" },
  "console.info":                         { "prefix": "ci",       "body": [ "console.info($1);" ],                             "description": "console.info" },
  "console.log":                          { "prefix": "clg",      "body": [ "console.log($1);" ],                              "description": "console.log" },
