  "console.trace":                        { "prefix": "ctr",      "body": [ "console.trace(${1:object});" ],                   "description": "console.trace" },

  "JSON.parse()":                         { "prefix": "jp",       "body": [ "JSON.parse(${1:obj});" ],     "description": "JSON.parse()" },
  "JSON.stringify()":                     { "prefix": "js",       "body": [ "JSON.stringify(${1:obj});" ], "description": "JSON.stringify()" },

  "try/catch":                            { "prefix": "tc",       "body": [ "try {\n\t${0}\n} catch (${1:err}) {\n\t\n}" ],                        "description": "try/catch" },
  "try/catch/finally":                    { "prefix": "tcf",      "body": [ "try {\n\t${0}\n} catch (${1:err}) {\n\t\n} finally {\n\t\n}" ],       "description": "try/catch/finally" },
  "try/finally":                          { "prefix": "tf",       "body": [ "try {\n\t${0}\n} finally {\n\t\n}" ],                                 "description": "try/finally" },
