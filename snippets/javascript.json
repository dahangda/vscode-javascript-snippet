  "var destructing array":                { "prefix": "vdea",     "body": [ "var [${2}] = ${1}${0};" ],                        "description": "var destructuring assignment" },

  "if":                                   { "prefix": "if",       "body": [ "if (${0:condition}) {\n\t${1}\n}" ],                                  "description": "if" },
  "if/else":                              { "prefix": "ife",      "body": [ "if (${0:condition}) {\n\t${1}\n} else {\n\t${2}\n}" ],                "description": "if/else" },
  "else if":                              { "prefix": "ei",       "body": [ "else if (${0:condition}) {\n\t${1}\n}" ],                             "description": "else if" },
  "else":                                 { "prefix": "el",       "body": [ "else {\n\t${0}\n}" ],                                                 "description": "else" },
  "switch case":                          { "prefix": "swi",      "body": [ "switch (${0}) {\n\tcase ${1} :\n\t\t${2}\n\t\tbreak;\n\tdefault:\n\t\tbreak;\n}" ], "description": "switch case" },
  "case":                                 { "prefix": "cas",      "body": [ "case ${0} :\n\t${1}\n\t\tbreak;" ],                                    "description": "switch's case" },

  "for of":                               { "prefix": "fof",      "body": [ "for (let ${1:item} of ${2:object}) {\n\t${0}\n}" ],                   "description": "Iterating over property names of iterable objects" },
  "for in":                               { "prefix": "fin",      "body": [ "for (let ${1:key} in ${2:object}) {\n\tif (${2:object}.hasOwnProperty(${1:key})) {\n\t\t${0}\n\t}\n}" ], "description": "Iterating over property values of iterable objects" },
  "for loop iterable":                    { "prefix": "fl",       "body": [ "for (let ${1:i} = 0, ${2:len} = ${3:iterable}.length; ${1:i} < ${2:len}; ${1:i}++) {\n\t${0}\n}" ],      "description": "for loop iterable" },
  "for loop iterable(reverse)":           { "prefix": "flr",      "body": [ "for (let ${0} = ${1}.length - 1; ${0} >= 0; ${0}--) {\n\t${2}\n}" ],  "description": "reverse for loop iterable" },
  "while iteration":                      { "prefix": "wh",       "body": [ "while (${1:condition}) {\n\t${0}\n}" ],                               "description": "while iteration" },
  "while iteration decrementing":         { "prefix": "whi",      "body": [ "let ${1:iterable}Index = ${1:array}.length\nwhile (${1:iterableIndex}Index--) {\n\t${0}\n}" ],           "description": "while iteration decrementing" },

  "Array.isArray()":                      { "prefix": "ia",       "body": [ "Array.isArray(${1:source})" ],                          "description": "Array.isArray()" },
  "array.every":                          { "prefix": "every",    "body": [ "${1:iterable}.every((${2:item}) => {\n\t${3}\n})" ],    "description": "array.every" },
  "array.filter":                         { "prefix": "filter",   "body": [ "${1:iterable}.filter((${2:item}) => {\n\t${3}\n})" ],   "description": "array.filter" },
  "array.find":                           { "prefix": "find",     "body": [ "${1:iterable}.find((${2:item}) => {\n\t${3}\n})" ],     "description": "array.find" },
  "array.some":                           { "prefix": "some",     "body": [ "${1:iterable}.some((${2:item}) => {\n\t${3}\n})" ],     "description": "array.some" },
  "array.map":                            { "prefix": "map",      "body": [ "${1:iterable}.map((${2:item}) => {\n\t${3}\n})" ],      "description": "array.map" },
  "array.forEach":                        { "prefix": "fe",       "body": [ "${1:iterable}.forEach(function(${2:item}) {\n\t${3}\n});" ], "description": "array.forEach" },
  "array.forEach arrow function":         { "prefix": "fea",      "body": [ "${1:iterable}.forEach((${2:item}) => {\n\t${3}\n});" ], "description": "array.forEach arrow function" },
  "array.reduce":                         { "prefix": "reduce",   "body": [ "${1:iterable}.reduce((${2:accumulator}, ${3:currentValue}) => {\n\t${3}\n}${4:, initialValue})" ], "description": "array.reduce" },
  "sequence of 0..n":                     { "prefix": "seq",      "body": [ "[...Array(${1:length}).keys()]${2}" ],                  "description": "sequence of 0..n" },

  "function":                             { "prefix": "fn",       "body": [ "function ${1:name} (${2:params}) {\n\t${3}\n}" ],                "description": "Creates function" },
  "function apply":                       { "prefix": "fnapply",  "body": [ "${1:method}.apply(${2:context}, [${3:params}]);" ],              "description": "function apply" },
  "function bind":                        { "prefix": "fnbind",   "body": [ "${1}.bind(${2:this})${0}" ],                                     "description": "function bind" },
  "function as a property of an object":  { "prefix": "fno",      "body": [ "${1:name}: function(${2:params}) {\n\t${3}\n}" ],                "description": "function as a property of an object" },
  "function call":                        { "prefix": "fncall",   "body": [ "${1:method}.call(${2:context}, ${3:params});" ],                 "description": "function call" },
  "anonymous function":                   { "prefix": "fna",      "body": [ "function (${1:params}) {${0}}" ],                                "description": "anonymous function" },
  "anonymous arrow function":             { "prefix": "af",       "body": [ "(${0:params}) => {\n\t${1}\n}" ],                                "description": "anonymous arrow function" },
  "arrow function with destructuring":    { "prefix": "afd",      "body": [ "({${0:params}}) => ${1:statement}" ],                            "description": "arrow function with destructuring" },
  "arrow function named":                 { "prefix": "afn",      "body": [ "const ${0:name} = (${1:params}) => {\n\t${2}\n}" ],              "description": "Creates a named arrow function" },
  "arrow function with return":           { "prefix": "afr",      "body": [ "(${0:params}) => {\n\treturn ${1}\n}" ],                         "description": "arrow function with return" },
  "generator function":                   { "prefix": "gf",       "body": [ "function* ${0:name}(${1:params}) {\n\t${2}\n}" ],                "description": "Creates generator function" },
  "yield":                                { "prefix": "y",        "body": [ "yield ${0}" ] },
  "iife":                                 { "prefix": "iif",      "body": [ ";(function (${1:params}) {\n\t${0}\n})(${2})" ],                 "description": "immediately-invoked function expression (IIFE)" },
  "iife, params":                         { "prefix": "iife",     "body": [ ";(function(${1:window}, ${2:document}) {\n\t${3}\n})(${1:window}, ${2:document})" ], "description": "immediately-invoked function expression (IIFE)" },

  "object literal":                       { "prefix": "ol",       "body": [ "{ ${1}: ${2}, }${0}" ],                                          "description": "object literal" },
  "key/value pair":                       { "prefix": "kv",       "body": [ "${1}: ${2}," ],                                                  "description": "key/value pair" },
  "Object.assign":                        { "prefix": "oa",       "body": [ "Object.assign(${1:target}, ${2:source})" ],                      "description": "Object.assign" },
  "Object.create":                        { "prefix": "oc",       "body": [ "Object.create(${1:obj})" ],                                      "description": "Object.create" },
  "Object.defineProperty":                { "prefix": "od",       "body": [ "Object.defineProperty(${1:dest}, '${2:prop}', {\n\t${0}\n})" ],  "description": "Object.defineProperty" },
  "Object.getOwnPropertyDescriptor":      { "prefix": "og",       "body": [ "Object.getOwnPropertyDescriptor(${1:obj}, '${2:prop}')" ],       "description": "Object.getOwnPropertyDescriptor" },
  "Object.keys":                          { "prefix": "ok",       "body": [ "Object.keys(${1:obj})" ],                                        "description": "Object.keys" },

  "typeof":                               { "prefix": "to",       "body": [ "typeof ${1:object} === '${2:undefined}'" ],                      "description": "typeof" },
  "instanceof":                           { "prefix": "io",       "body": [ "${1:object} instanceof '${2:constructor}'" ],                    "description": "instanceof" },

  "setTimeout":                           { "prefix": "st",       "body": [ "setTimeout(function() {\n\t${0}\n}, ${1:delay});" ],     "description": "setTimeout" },
  "setTimeOut, arrow function":           { "prefix": "sta",      "body": [ "setTimeout(() => {\n\t${2}\n}, ${1:delay});" ],          "description": "setTimeOut with arrow function" },
