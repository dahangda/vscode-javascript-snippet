  "export class":                         { "prefix": "ecl",      "body": [ "export default class ${1:className} {\n\t$0\n};\n" ],               "description": "export class" },
  "export class extends":                 { "prefix": "ece",      "body": [ "export default class ${1:className} extends ${2:baseClassName} {\n\t$0\n};\n" ], "description": "export class extends" },
  "export default arrow function":        { "prefix": "edaf",     "body": [ "export default (${0:params}) => {\n\t$1\n};\n" ],                  "description": "export default arrow function" },
