{
  "use strict":                           { "prefix": "us",       "body": [ "'use strict';" ],                                 "description": "use strict" },
  "const":                                { "prefix": "c",        "body": [ "const ${1:name} = ${0:value};" ],                 "description": "const assignment" },
  "const awaited":                        { "prefix": "caw",      "body": [ "const ${1:name} = await ${2:value};" ],           "description": "const assignment awaited" },
  "const yielded":                        { "prefix": "cy",       "body": [ "const ${1:name} = yield ${2:value};" ],           "description": "const assignment yielded" },
  "const destructing object":             { "prefix": "cde",      "body": [ "const {${2}} = ${1}${0};" ],                      "description": "const assignment destructuring" },
  "const destructing array":              { "prefix": "cdea",     "body": [ "const [${2}] = ${1}${0};" ],                      "description": "const assignment destructuring" },
  "let":                                  { "prefix": "l",        "body": [ "let ${1:name} = ${0:value};" ],                   "description": "let assignment" },
  "let awaited":                          { "prefix": "law",      "body": [ "let ${1:name} = await ${2:value};" ],             "description": "let assignment awaited" },
  "let yielded":                          { "prefix": "ly",       "body": [ "let ${1:name} = yield ${2:value};" ],             "description": "let assignment yielded" },
